              </div>
                <span class="text-white mx-2"><i class="fas fa-map-marker-alt"></i> Hồ Chí Minh</span>
                <a href="tel:19001533" class="btn btn-danger fw-bold my-1">
                  <i class="fas fa-phone-alt"></i> 1900-1533
                </a>
                <a href="mon-moi-mon-ngon.html" class="btn btn-warning fw-bold ms-2 my-1">PICK UP</a>
              </div>
            </div>
            <!-- Right: Actions (desktop) -->
            <div class="d-none d-lg-flex align-items-center gap-3">
              <div class="user-actions d-flex gap-3">
                @if (Context.Session.GetString("IsUserLoggedIn") == "true")
                {
                  <div class="dropdown">
                    <a href="#" class="user-profile-btn d-flex align-items-center text-decoration-none" id="userDropdownDesktop" data-bs-toggle="dropdown" aria-expanded="false">
                      <div class="user-avatar me-2">
                        <i class="fas fa-user"></i>
                      </div>
                      <span class="user-name text-white fw-medium">@Context.Session.GetString("UserName")</span>
                      <i class="fas fa-chevron-down ms-2 text-white" style="font-size: 0.8rem;"></i>
                    </a>
                    <ul class="dropdown-menu dropdown-menu-end user-dropdown-menu" aria-labelledby="userDropdownDesktop">
                      <li class="user-dropdown-header">
                        <div class="d-flex align-items-center p-3 bg-light">
                          <div class="user-avatar-large me-3">
                            <i class="fas fa-user"></i>
                          </div>
                          <div>
                            <div class="fw-bold text-dark">@Context.Session.GetString("UserName")</div>
                            <small class="text-muted">@Context.Session.GetString("UserEmail")</small>
                          </div>
                        </div>
                      </li>
                      <li><a class="dropdown-item py-2" href="#"><i class="fas fa-user me-3 text-muted"></i>Tài khoản của tôi</a></li>
                      <li><a class="dropdown-item py-2" href="#"><i class="fas fa-shopping-bag me-3 text-muted"></i>Đơn hàng của tôi</a></li>
                      <li><a class="dropdown-item py-2" href="#"><i class="fas fa-heart me-3 text-muted"></i>Yêu thích</a></li>
                      <li><a class="dropdown-item py-2" href="#"><i class="fas fa-gift me-3 text-muted"></i>Ưu đãi của tôi</a></li>
                      <li><hr class="dropdown-divider my-1"></li>
                      <li>
                        <form asp-controller="Account" asp-action="Logout" method="post" class="d-inline w-100">
                          @Html.AntiForgeryToken()
                          <button type="submit" class="dropdown-item py-2 logout-btn border-0 bg-transparent w-100 text-start">
                            <i class="fas fa-sign-out-alt me-3 text-danger"></i>Đăng xuất
                          </button>
                        </form>
                      </li>
                    </ul>
                  </div>
                }
                else
                {
                  <a asp-controller="Account" asp-action="Login" class="icon-btn" title="Đăng nhập" data-bs-toggle="tooltip" data-bs-placement="bottom">
                    <i class="fas fa-user"></i>
                  </a>
                  <a asp-controller="Account" asp-action="Register" class="icon-btn" title="Đăng ký" data-bs-toggle="tooltip" data-bs-placement="bottom">
                    <i class="fas fa-user-plus"></i>
                  </a>
                }
              </div>
              <span class="text-white mx-2"><i class="fas fa-map-marker-alt"></i> Hồ Chí Minh</span>
              <a href="tel:19001533" class="btn btn-danger fw-bold">
                <i class="fas fa-phone-alt"></i> 1900-1533
              </a>
              <a href="mon-moi-mon-ngon.html" class="btn btn-warning fw-bold ms-2">PICK UP</a>
            </div>
          </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="jolli-footer mt-5 pt-5">
