                <li class="nav-item"><a class="nav-link text-white fw-bold" href="https://jollibee.talent.vn/">Tuyển dụng</a></li>
              </ul>
              <!-- Right: Actions (mobile) -->
              <div class="d-lg-none mt-3">
                              <div class="user-actions d-flex gap-3 mb-3">
                @if (Context.Session.GetString("IsUserLoggedIn") == "true")
                {
                  <div class="dropdown">
                    <a href="#" class="user-profile-btn d-flex align-items-center text-decoration-none" id="userDropdownMobile" data-bs-toggle="dropdown" aria-expanded="false">
